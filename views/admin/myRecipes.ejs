<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Minhas Receitas</title>
</head>
<body>
  <ul>
    <h1>Receitas</h1>
    <% recipes.forEach(({user, recipe, id}) => { %>
      <li>
        <%=`${recipe} ${user}`%>
        <a href=<%= `recipes/${id}` %> >Ver mais</a>
      </li>
    <% }) %>
  </ul>
</body>
</html>
